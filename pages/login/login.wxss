.index-block{
  position: absolute;
  width: 100%;
  min-width: 100%;
  min-height: 100%;
}
.logo{
  /* position: absolute; */
  width: 100%;
  /* height: 500rpx; */
}
.login-window{
  width: 100%;
  /* margin-top: 45%; */
  height: 300rpx;
  
}
view.search-login {
  flex-direction: row;
  align-items: center;
  
}

view.searchItem-login {
  display: flex;
  width: 95%;
  height: 80rpx;
  flex-direction: row;
  margin: 0 auto;
  border: 1rpx rgb(236, 233, 233) solid;
}

view.searchItem-login image {
  height: 42rpx;
  width: 42rpx;
  align-self: center;
  margin-left: 10rpx;
  margin-right: 20rpx;
}

view.searchItem-login input {
  flex: 1;
  font-size: 30rpx;
  color: #000;
  width: 100%;
  height: 80rpx;
}
.login{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #15D4B3;
  text-align: center;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #fff;
}
.login-1{
  
  margin-top: 20rpx;

}
.login-1 view{
  text-align: center;
  width: 48%;
  height: 80rpx;
  line-height: 80rpx;
  background: silver;
  display: inline-block;
  border-radius: 10rpx;
  font-size: 28rpx;
}
.slogan{
  margin-top: 20rpx;
  font-size: 30rpx;
  color:  rgb(0, 73, 133);
  text-align: center;
}