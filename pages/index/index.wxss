
.index-logo{
  width: 100%;
  height: 200rpx;
  
  padding-top: 120rpx;
  
}
.index-logo view{
  margin: 0 auto;
  width: 140rpx;
  height: 140rpx;
  /* background: #EAEAE8; */
   border-radius: 130rpx;
  box-shadow: 0px 0px 10px 5px rgba(192, 192, 192, 0.9); 
  
}
.index-logo text{
  text-align: center;
  margin-top: 20rpx;

  font-size: 30rpx;
  display: block;
  
}
.index-logo view image{
  width: 140rpx;
  height: 140rpx;
  
 
}
.index-middle{
  /* width: 95%; */
  height: 50rpx;
  text-align: center;
  margin: 0 auto
}
.index-middle view{
  margin-top: 50rpx;
  margin-bottom: 15rpx;
  width: 33%;
  height: 80rpx;
  border-right: 1rpx rgb(255, 255, 255) solid;
  float: left;
  font-size: 28rpx;
}
.index-middle view text{
  display: block;
}
.index-app{
  /* width: 95%; */
  margin:  0 auto;
}
.index-app-mod{
  width: 33%;
  background: #fff;
  border-right: 1rpx rgb(238, 238, 238) solid;
  border-bottom: 1rpx rgb(238, 238, 238) solid;
  height: 250rpx;
  
  float: left;
}
.index-app-mod view{
  margin-top: 80rpx;
}
.none-style{
  border-right: 0rpx;
}
.index-app-mod image{
  width: 50rpx;
  height: 50rpx;
  display: block;
  top: 0;
  left: 0 ;
  right: 0;
  bottom: 0;
  margin: auto;
}
.index-app-mod text{
  text-align: center;
  display: block;
  margin-top: 20rpx;
  font-size: 24rpx;
  

}